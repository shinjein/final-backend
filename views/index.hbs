<h1>Home Feed</h1>
<hr>
<div>
  {{#each recipesFromDB}}
    <h3>
      {{title}}
    </h3>
        <img src="{{imageUrl}}" alt="" style="height: 200px; width: 200px;">
        <p>{{description}}</p>
        <p>posted by: {{user}}</p>
      <form action="/recipe-details/{{id}}" method="GET">
      <button type="submit">
        Recipe Details
        </button>
      </form>
  {{/each}}
</div>

<hr>